      { "x": 5, "y": 7, "panel_id": "P2", "line_ids": ["L15"], "payload_text": "L15 Fair youth, beneath the trees, thou canst not leave", "beacon": false, "redundancy_role": "core" },
      { "x": 6, "y": 7, "panel_id": "P2", "line_ids": ["L16"], "payload_text": "L16 Thy song, nor ever can those trees be bare;", "beacon": false, "redundancy_role": "core" },
      { "x": 7, "y": 7, "panel_id": "P2", "line_ids": ["L17"], "payload_text": "L17 Bold Lover, never, never canst thou kiss,", "beacon": false, "redundancy_role": "core" },
      { "x": 8, "y": 7, "panel_id": "P2", "line_ids": ["L18"], "payload_text": "L18 Though winning near the goal yet, do not grieve;", "beacon": false, "redundancy_role": "core" },
      { "x": 9, "y": 7, "panel_id": "P2", "line_ids": ["L19"], "payload_text": "L19 She cannot fade, though thou hast not thy bliss,", "beacon": false, "redundancy_role": "core" },
      { "x": 10, "y": 7, "panel_id": "P2", "line_ids": ["L20"], "payload_text": "L20 For ever wilt thou love, and she be fair!", "beacon": false, "redundancy_role": "core" },

      { "x": 1, "y": 8, "panel_id": "P2", "line_ids": ["L11","L12"], "payload_text": "L11–L12 (unheard sweeter; play on)", "beacon": false, "redundancy_role": "echo" },
      { "x": 2, "y": 8, "panel_id": "P2", "line_ids": ["L13","L14"], "payload_text": "L13–L14 (sense→spirit channel)", "beacon": false, "redundancy_role": "echo" },
      { "x": 3, "y": 8, "panel_id": "P2", "line_ids": ["L15","L16"], "payload_text": "L15–L16 (youth/trees/song fixed)", "beacon": false, "redundancy_role": "echo" },
      { "x": 4, "y": 8, "panel_id": "P2", "line_ids": ["L17","L18"], "payload_text": "L17–L18 (kiss deferred; no grieve)", "beacon": false, "redundancy_role": "echo" },
      { "x": 5, "y": 8, "panel_id": "P2", "line_ids": ["L19","L20"], "payload_text": "L19–L20 (no fade; forever fair)", "beacon": false, "redundancy_role": "echo" },

      { "x": 0, "y": 10, "panel_id": "P2", "line_ids": [], "payload_text": "§B|P2|LOCK|UNHEARD_AUDIO|7C", "beacon": true, "redundancy_role": "core" },
      { "x": 1, "y": 9, "panel_id": "P2", "line_ids": ["L12"], "payload_text": "XREF: question-music => P1(L10)", "beacon": false, "redundancy_role": "glue" },
      { "x": 2, "y": 9, "panel_id": "P2", "line_ids": ["L20"], "payload_text": "XREF: 'for ever' recursion => P3(L24–L27)", "beacon": false, "redundancy_role": "glue" },

      { "x": 1, "y": 11, "panel_id": "P3", "line_ids": ["L21"], "payload_text": "L21 Ah, happy, happy boughs! that cannot shed", "beacon": false, "redundancy_role": "core" },
      { "x": 2, "y": 11, "panel_id": "P3", "line_ids": ["L22"], "payload_text": "L22 Your leaves, nor ever bid the Spring adieu;", "beacon": false, "redundancy_role": "core" },
      { "x": 3, "y": 11, "panel_id": "P3", "line_ids": ["L23"], "payload_text": "L23 And, happy melodist, unwearied,", "beacon": false, "redundancy_role": "core" },
      { "x": 4, "y": 11, "panel_id": "P3", "line_ids": ["L24"], "payload_text": "L24 For ever piping songs for ever new;", "beacon": false, "redundancy_role": "core" },
      { "x": 5, "y": 11, "panel_id": "P3", "line_ids": ["L25"], "payload_text": "L25 More happy love! more happy, happy love!", "beacon": false, "redundancy_role": "core" },
      { "x": 6, "y": 11, "panel_id": "P3", "line_ids": ["L26"], "payload_text": "L26 For ever warm and still to be enjoy'd,", "beacon": false, "redundancy_role": "core" },
      { "x": 7, "y": 11, "panel_id": "P3", "line_ids": ["L27"], "payload_text": "L27 For ever panting, and for ever young;", "beacon": false, "redundancy_role": "core" },
      { "x": 8, "y": 11, "panel_id": "P3", "line_ids": ["L28"], "payload_text": "L28 All breathing human passion far above,", "beacon": false, "redundancy_role": "core" },
      { "x": 9, "y": 11, "panel_id": "P3", "line_ids": ["L29"], "payload_text": "L29 That leaves a heart high-sorrowful and cloy'd,", "beacon": false, "redundancy_role": "core" },
      { "x": 10, "y": 11, "panel_id": "P3", "line_ids": ["L30"], "payload_text": "L30 A burning forehead, and a parching tongue.", "beacon": false, "redundancy_role": "core" },

      { "x": 1, "y": 12, "panel_id": "P3", "line_ids": ["L21","L22"], "payload_text": "L21–L22 (cannot shed; Spring suspended)", "beacon": false, "redundancy_role": "echo" },
      { "x": 2, "y": 12, "panel_id": "P3", "line_ids": ["L23","L24"], "payload_text": "L23–L24 (unwearied; forever new)", "beacon": false, "redundancy_role": "echo" },
      { "x": 3, "y": 12, "panel_id": "P3", "line_ids": ["L25","L26"], "payload_text": "L25–L26 (happy love; forever warm)", "beacon": false, "redundancy_role": "echo" },
      { "x": 4, "y": 12, "panel_id": "P3", "line_ids": ["L27","L28"], "payload_text": "L27–L28 (forever young; passion above)", "beacon": false, "redundancy_role": "echo" },
      { "x": 5, "y": 12, "panel_id": "P3", "line_ids": ["L29","L30"], "payload_text": "L29–L30 (cloy + burning/parching)", "beacon": false, "redundancy_role": "echo" },

      { "x": 0, "y": 14, "panel_id": "P3", "line_ids": [], "payload_text": "§B|P3|LOCK|AFFECT_FOREVER|55", "beacon": true, "redundancy_role": "core" },
      { "x": 1, "y": 13, "panel_id": "P3", "line_ids": ["L24"], "payload_text": "ANCHOR: token=FOR_EVER (high recall)", "beacon": false, "redundancy_role": "glue" },
      { "x": 2, "y": 13, "panel_id": "P3", "line_ids": ["L29","L30"], "payload_text": "XREF: passion→cost => P5(COLD_PASTORAL)", "beacon": false, "redundancy_role": "glue" },

      { "x": 1, "y": 15, "panel_id": "P4", "line_ids": ["L31"], "payload_text": "L31 Who are these coming to the sacrifice?", "beacon": false, "redundancy_role": "core" },
      { "x": 2, "y": 15, "panel_id": "P4", "line_ids": ["L32"], "payload_text": "L32 To what green altar, O mysterious priest,", "beacon": false, "redundancy_role": "core" },
      { "x": 3, "y": 15, "panel_id": "P4", "line_ids": ["L33"], "payload_text": "L33 Lead'st thou that heifer lowing at the skies,", "beacon": false, "redundancy_role": "core" },
      { "x": 4, "y": 15, "panel_id": "P4", "line_ids": ["L34"], "payload_text": "L34 And all her silken flanks with garlands drest?", "beacon": false, "redundancy_role": "core" },
      { "x": 5, "y": 15, "panel_id": "P4", "line_ids": ["L35"], "payload_text": "L35 What little town by river or sea shore,", "beacon": false, "redundancy_role": "core" },
      { "x": 6, "y": 15, "panel_id": "P4", "line_ids": ["L36"], "payload_text": "L36 Or mountain-built with peaceful citadel,", "beacon": false, "redundancy_role": "core" },
      { "x": 7, "y": 15, "panel_id": "P4", "line_ids": ["L37"], "payload_text": "L37 Is emptied of this folk, this pious morn?", "beacon": false, "redundancy_role": "core" },
      { "x": 8, "y": 15, "panel_id": "P4", "line_ids": ["L38"], "payload_text": "L38 And, little town, thy streets for evermore", "beacon": false, "redundancy_role": "core" },
      { "x": 9, "y": 15, "panel_id": "P4", "line_ids": ["L39"], "payload_text": "L39 Will silent be; and not a soul to tell", "beacon": false, "redundancy_role": "core" },
      { "x": 10, "y": 15, "panel_id": "P4", "line_ids": ["L40"], "payload_text": "L40 Why thou art desolate, can e'er return.", "beacon": false, "redundancy_role": "core" },

      { "x": 1, "y": 16, "panel_id": "P4", "line_ids": ["L31","L32"], "payload_text": "L31–L32 (who? altar? priest)", "beacon": false, "redundancy_role": "echo" },
      { "x": 2, "y": 16, "panel_id": "P4", "line_ids": ["L33","L34"], "payload_text": "L33–L34 (heifer + garlands)", "beacon": false, "redundancy_role": "echo" },
      { "x": 3, "y": 16, "panel_id": "P4", "line_ids": ["L35","L36"], "payload_text": "L35–L36 (town by river/sea; citadel)", "beacon": false, "redundancy_role": "echo" },
      { "x": 4, "y": 16, "panel_id": "P4", "line_ids": ["L37","L38"], "payload_text": "L37–L38 (emptied; streets forever)", "beacon": false, "redundancy_role": "echo" },
      { "x": 5, "y": 16, "panel_id": "P4", "line_ids": ["L39","L40"], "payload_text": "L39–L40 (silence; no return)", "beacon": false, "redundancy_role": "echo" },

      { "x": 0, "y": 18, "panel_id": "P4", "line_ids": [], "payload_text": "§B|P4|LOCK|RITUAL_EMPTY_TOWN|2E", "beacon": true, "redundancy_role": "core" },
      { "x": 1, "y": 17, "panel_id": "P4", "line_ids": ["L39","L40"], "payload_text": "ANCHOR: token=NO_RETURN (closure handle)", "beacon": false, "redundancy_role": "glue" },
      { "x": 2, "y": 17, "panel_id": "P4", "line_ids": ["L02"], "payload_text": "XREF: silence/time => P1(L02)", "beacon": false, "redundancy_role": "glue" },

      { "x": 1, "y": 19, "panel_id": "P5", "line_ids": ["L41"], "payload_text": "L41 O Attic shape! Fair attitude! with brede", "beacon": false, "redundancy_role": "core" },
      { "x": 2, "y": 19, "panel_id": "P5", "line_ids": ["L42"], "payload_text": "L42 Of marble men and maidens overwrought,", "beacon": false, "redundancy_role": "core" },
      { "x": 3, "y": 19, "panel_id": "P5", "line_ids": ["L43"], "payload_text": "L43 With forest branches and the trodden weed;", "beacon": false, "redundancy_role": "core" },
      { "x": 4, "y": 19, "panel_id": "P5", "line_ids": ["L44"], "payload_text": "L44 Thou, silent form, dost tease us out of thought", "beacon": false, "redundancy_role": "core" },
      { "x": 5, "y": 19, "panel_id": "P5", "line_ids": ["L45"], "payload_text": "L45 As doth eternity: Cold Pastoral!", "beacon": false, "redundancy_role": "core" },
      { "x": 6, "y": 19, "panel_id": "P5", "line_ids": ["L46"], "payload_text": "L46 When old age shall this generation waste,", "beacon": false, "redundancy_role": "core" },
      { "x": 7, "y": 19, "panel_id": "P5", "line_ids": ["L47"], "payload_text": "L47 Thou shalt remain, in midst of other woe", "beacon": false, "redundancy_role": "core" },
      { "x": 8, "y": 19, "panel_id": "P5", "line_ids": ["L48"], "payload_text": "L48 Than ours, a friend to man, to whom thou say'st,", "beacon": false, "redundancy_role": "core" },
      { "x": 9, "y": 19, "panel_id": "P5", "line_ids": ["L49"], "payload_text": "L49 \"Beauty is truth, truth beauty,—that is all", "beacon": false, "redundancy_role": "core" },
      { "x": 10, "y": 19, "panel_id": "P5", "line_ids": ["L50"], "payload_text": "L50 Ye know on earth, and all ye need to know.\"", "beacon": false, "redundancy_role": "core" },

      { "x": 1, "y": 20, "panel_id": "P5", "line_ids": ["L41","L42"], "payload_text": "L41–L42 (Attic shape; marble figures)", "beacon": false, "redundancy_role": "echo" },
      { "x": 2, "y": 20, "panel_id": "P5", "line_ids": ["L43","L44"], "payload_text": "L43–L44 (branches/weed; silent tease)", "beacon": false, "redundancy_role": "echo" },
      { "x": 3, "y": 20, "panel_id": "P5", "line_ids": ["L45","L46"], "payload_text": "L45–L46 (eternity; generation waste)", "beacon": false, "redundancy_role": "echo" },
      { "x": 4, "y": 20, "panel_id": "P5", "line_ids": ["L47","L48"], "payload_text": "L47–L48 (remain; friend to man; says)", "beacon": false, "redundancy_role": "echo" },
      { "x": 5, "y": 20, "panel_id": "P5", "line_ids": ["L49","L50"], "payload_text": "L49–L50 (EPIGRAM)", "beacon": false, "redundancy_role": "echo" },

