  "line_index": [
    { "line_id": "L01", "stanza": 1, "line_number": 1, "text": "Thou still unravish'd bride of quietness,", "motifs": ["address", "silence"], "entities": ["urn", "bride"], "actions": ["address"] },
    { "line_id": "L02", "stanza": 1, "line_number": 2, "text": "Thou foster-child of silence and slow time,", "motifs": ["silence", "time"], "entities": ["urn", "foster-child"], "actions": ["address"] },
    { "line_id": "L03", "stanza": 1, "line_number": 3, "text": "Sylvan historian, who canst thus express", "motifs": ["history", "inscription"], "entities": ["urn", "historian"], "actions": ["express"] },
    { "line_id": "L04", "stanza": 1, "line_number": 4, "text": "A flowery tale more sweetly than our rhyme:", "motifs": ["comparison", "tale"], "entities": ["tale", "rhyme"], "actions": ["tell", "compare"] },
    { "line_id": "L05", "stanza": 1, "line_number": 5, "text": "What leaf-fring'd legend haunts about thy shape", "motifs": ["legend", "haunting", "questions"], "entities": ["legend", "shape"], "actions": ["haunt", "ask"] },
    { "line_id": "L06", "stanza": 1, "line_number": 6, "text": "Of deities or mortals, or of both,", "motifs": ["myth", "mixed beings"], "entities": ["deities", "mortals"], "actions": ["classify"] },
    { "line_id": "L07", "stanza": 1, "line_number": 7, "text": "In Tempe or the dales of Arcady?", "motifs": ["place", "pastoral"], "entities": ["Tempe", "Arcady"], "actions": ["locate", "ask"] },
    { "line_id": "L08", "stanza": 1, "line_number": 8, "text": "What men or gods are these? What maidens loth?", "motifs": ["questions", "scene decoding"], "entities": ["men", "gods", "maidens"], "actions": ["ask", "identify"] },
    { "line_id": "L09", "stanza": 1, "line_number": 9, "text": "What mad pursuit? What struggle to escape?", "motifs": ["chase", "frozen action"], "entities": ["pursuit", "struggle"], "actions": ["pursue", "escape", "ask"] },
    { "line_id": "L10", "stanza": 1, "line_number": 10, "text": "What pipes and timbrels? What wild ecstasy?", "motifs": ["music", "unheard audio"], "entities": ["pipes", "timbrels", "ecstasy"], "actions": ["play", "ask"] },

    { "line_id": "L11", "stanza": 2, "line_number": 1, "text": "Heard melodies are sweet, but those unheard", "motifs": ["heard/unheard", "audio channel"], "entities": ["melodies"], "actions": ["contrast"] },
    { "line_id": "L12", "stanza": 2, "line_number": 2, "text": "Are sweeter; therefore, ye soft pipes, play on;", "motifs": ["unheard audio", "loop command"], "entities": ["pipes"], "actions": ["command", "play"] },
    { "line_id": "L13", "stanza": 2, "line_number": 3, "text": "Not to the sensual ear, but, more endear'd,", "motifs": ["spirit vs sense"], "entities": ["ear"], "actions": ["redirect"] },
    { "line_id": "L14", "stanza": 2, "line_number": 4, "text": "Pipe to the spirit ditties of no tone:", "motifs": ["unheard audio", "spirit channel"], "entities": ["spirit", "ditties"], "actions": ["pipe"] },
    { "line_id": "L15", "stanza": 2, "line_number": 5, "text": "Fair youth, beneath the trees, thou canst not leave", "motifs": ["frozen action", "eternity"], "entities": ["youth", "trees"], "actions": ["cannot leave"] },
    { "line_id": "L16", "stanza": 2, "line_number": 6, "text": "Thy song, nor ever can those trees be bare;", "motifs": ["permanence", "perpetual nature"], "entities": ["song", "trees"], "actions": ["sing", "remain"] },
    { "line_id": "L17", "stanza": 2, "line_number": 7, "text": "Bold Lover, never, never canst thou kiss,", "motifs": ["desire deferred", "frozen climax"], "entities": ["lover"], "actions": ["cannot kiss"] },
    { "line_id": "L18", "stanza": 2, "line_number": 8, "text": "Though winning near the goal yet, do not grieve;", "motifs": ["near-goal", "instruction"], "entities": ["goal"], "actions": ["console", "instruct"] },
    { "line_id": "L19", "stanza": 2, "line_number": 9, "text": "She cannot fade, though thou hast not thy bliss,", "motifs": ["anti-decay", "tradeoff"], "entities": ["she", "bliss"], "actions": ["cannot fade"] },
    { "line_id": "L20", "stanza": 2, "line_number": 10, "text": "For ever wilt thou love, and she be fair!", "motifs": ["forever", "loop"], "entities": ["thou", "she"], "actions": ["love", "remain fair"] },

    { "line_id": "L21", "stanza": 3, "line_number": 1, "text": "Ah, happy, happy boughs! that cannot shed", "motifs": ["perpetual spring", "cannot"], "entities": ["boughs"], "actions": ["cannot shed"] },
    { "line_id": "L22", "stanza": 3, "line_number": 2, "text": "Your leaves, nor ever bid the Spring adieu;", "motifs": ["forever", "season suspended"], "entities": ["leaves", "Spring"], "actions": ["cannot bid adieu"] },
    { "line_id": "L23", "stanza": 3, "line_number": 3, "text": "And, happy melodist, unwearied,", "motifs": ["unwearied loop"], "entities": ["melodist"], "actions": ["persist"] },
    { "line_id": "L24", "stanza": 3, "line_number": 4, "text": "For ever piping songs for ever new;", "motifs": ["for ever", "recursion"], "entities": ["songs"], "actions": ["pipe"] },
    { "line_id": "L25", "stanza": 3, "line_number": 5, "text": "More happy love! more happy, happy love!", "motifs": ["affect amplification"], "entities": ["love"], "actions": ["exclaim"] },
    { "line_id": "L26", "stanza": 3, "line_number": 6, "text": "For ever warm and still to be enjoy'd,", "motifs": ["eternal enjoyment"], "entities": ["love"], "actions": ["be enjoyed"] },
    { "line_id": "L27", "stanza": 3, "line_number": 7, "text": "For ever panting, and for ever young;", "motifs": ["for ever", "youth loop"], "entities": ["panting", "young"], "actions": ["pant", "remain young"] },
    { "line_id": "L28", "stanza": 3, "line_number": 8, "text": "All breathing human passion far above,", "motifs": ["human passion", "excess"], "entities": ["passion"], "actions": ["breathe"] },
    { "line_id": "L29", "stanza": 3, "line_number": 9, "text": "That leaves a heart high-sorrowful and cloy'd,", "motifs": ["physiology", "cloy"], "entities": ["heart"], "actions": ["leave"] },
    { "line_id": "L30", "stanza": 3, "line_number": 10, "text": "A burning forehead, and a parching tongue.", "motifs": ["physiology", "burning/parching"], "entities": ["forehead", "tongue"], "actions": ["burn", "parch"] },

    { "line_id": "L31", "stanza": 4, "line_number": 1, "text": "Who are these coming to the sacrifice?", "motifs": ["ritual", "questions"], "entities": ["these", "sacrifice"], "actions": ["come", "ask"] },
    { "line_id": "L32", "stanza": 4, "line_number": 2, "text": "To what green altar, O mysterious priest,", "motifs": ["altar", "address"], "entities": ["altar", "priest"], "actions": ["address", "ask"] },
    { "line_id": "L33", "stanza": 4, "line_number": 3, "text": "Lead'st thou that heifer lowing at the skies,", "motifs": ["procession", "animal voice"], "entities": ["heifer", "skies"], "actions": ["lead", "low"] },
    { "line_id": "L34", "stanza": 4, "line_number": 4, "text": "And all her silken flanks with garlands drest?", "motifs": ["ornament", "ritual dressing"], "entities": ["flanks", "garlands"], "actions": ["dress"] },
    { "line_id": "L35", "stanza": 4, "line_number": 5, "text": "What little town by river or sea shore,", "motifs": ["place", "absence"], "entities": ["town", "river", "sea shore"], "actions": ["ask", "locate"] },
    { "line_id": "L36", "stanza": 4, "line_number": 6, "text": "Or mountain-built with peaceful citadel,", "motifs": ["citadel", "peace"], "entities": ["mountain", "citadel"], "actions": ["describe"] },
    { "line_id": "L37", "stanza": 4, "line_number": 7, "text": "Is emptied of this folk, this pious morn?", "motifs": ["emptied", "ritual morning"], "entities": ["folk", "morn"], "actions": ["empty"] },
    { "line_id": "L38", "stanza": 4, "line_number": 8, "text": "And, little town, thy streets for evermore", "motifs": ["forever", "desolation"], "entities": ["town", "streets"], "actions": ["address", "remain"] },
    { "line_id": "L39", "stanza": 4, "line_number": 9, "text": "Will silent be; and not a soul to tell", "motifs": ["silence", "no witness"], "entities": ["soul"], "actions": ["be silent"] },
    { "line_id": "L40", "stanza": 4, "line_number": 10, "text": "Why thou art desolate, can e'er return.", "motifs": ["no return", "closure"], "entities": ["thou", "desolate"], "actions": ["cannot return"] },

    { "line_id": "L41", "stanza": 5, "line_number": 1, "text": "O Attic shape! Fair attitude! with brede", "motifs": ["address", "artifact"], "entities": ["Attic shape", "attitude", "brede"], "actions": ["address"] },
    { "line_id": "L42", "stanza": 5, "line_number": 2, "text": "Of marble men and maidens overwrought,", "motifs": ["material", "engraving"], "entities": ["marble men", "maidens"], "actions": ["overwrought"] },
    { "line_id": "L43", "stanza": 5, "line_number": 3, "text": "With forest branches and the trodden weed;", "motifs": ["pastoral", "ground"], "entities": ["branches", "weed"], "actions": ["tread"] },
    { "line_id": "L44", "stanza": 5, "line_number": 4, "text": "Thou, silent form, dost tease us out of thought", "motifs": ["silence", "cognitive displacement"], "entities": ["silent form", "us"], "actions": ["tease"] },
    { "line_id": "L45", "stanza": 5, "line_number": 5, "text": "As doth eternity: Cold Pastoral!", "motifs": ["eternity", "cold pastoral"], "entities": ["eternity"], "actions": ["name"] },
    { "line_id": "L46", "stanza": 5, "line_number": 6, "text": "When old age shall this generation waste,", "motifs": ["time", "decay"], "entities": ["old age", "generation"], "actions": ["waste"] },
    { "line_id": "L47", "stanza": 5, "line_number": 7, "text": "Thou shalt remain, in midst of other woe", "motifs": ["remains", "archive endurance"], "entities": ["thou", "woe"], "actions": ["remain"] },
    { "line_id": "L48", "stanza": 5, "line_number": 8, "text": "Than ours, a friend to man, to whom thou say'st,", "motifs": ["address", "speech from object"], "entities": ["friend", "man"], "actions": ["say"] },
    { "line_id": "L49", "stanza": 5, "line_number": 9, "text": "\"Beauty is truth, truth beauty,—that is all", "motifs": ["epigram", "truth/beauty"], "entities": ["beauty", "truth"], "actions": ["assert"] },
    { "line_id": "L50", "stanza": 5, "line_number": 10, "text": "Ye know on earth, and all ye need to know.\"", "motifs": ["epigram", "closure"], "entities": ["ye", "earth"], "actions": ["close"] }
  ],

