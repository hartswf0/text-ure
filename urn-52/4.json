  "retrieval_ruleset": {
    "beacon_detection": {
      "predicate": "payload_text.startsWith('§')",
      "parse": "split by '|' into [§B, panel_id, beacon_type, label, crc]"
    },
    "panel_lock": {
      "lock_on_beacon_type": "LOCK",
      "unlock_on_beacon_type": "EXIT",
      "bias": {
        "prefer_same_panel": 3,
        "prefer_core_over_echo_over_glue": { "core": 3, "echo": 2, "glue": 1 },
        "penalty_for_crossing_panel_while_locked": 10
      }
    },
    "seek_cost_metric": {
      "formula": "abs(dx) + 2*abs(dy) + 6*panel_switches + 10*lock_breaks",
      "panel_switch_definition": "best-next-hit panel_id != current_or_active_panel"
    },
    "raycast_hit_definition": {
      "hit_if": "beacon==true OR payload_text.length>0",
      "max_steps": 48,
      "wrap_x": true
    }
  },

  "compiler_plan": {
    "goal": "Serialize cell contents into a single stream that preserves panel and line structure for lossless re-expansion.",
    "tokens": {
      "panel_seam": "⟂PANEL⟂",
      "cell_seam": "⟂CELL⟂",
      "line_break_glyph": "␤"
    },
    "beam_order": "for y=0..23: for x=0..23 emit cell record",
    "cell_record_format": "⟂CELL⟂@(x,y)|panel_id|redundancy_role|beacon|line_ids_csv|payload_text␤",
    "panel_record_format": "⟂PANEL⟂|panel_id|title|crc␤ ...cells... ⟂PANEL⟂|END|panel_id|crc␤",
    "structure_preservation": [
      "Keep line_ids in every cell that stores poem text.",
      "Keep explicit panel_id per cell to allow rebuild without geometry assumptions.",
      "Use seam column CRCs (x=23 cells) as resync points if stream is damaged."
    ]
  }
}
